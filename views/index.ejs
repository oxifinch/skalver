<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8" />
		<title>Skalver | Dashboard</title>
		<meta name="description" content="Dashboard of Skalver web app"/>
		<meta name="viewport" content="width=device-width, initial-scale=1"/>
	</head>

	<body>
        <main>
            <%- include ('partials/dashboard-header.ejs') %>

            <div id="dashboard_sections">
                <% sections.forEach((sec) => { %>
                <%- include('partials/library-section.ejs', {sec: sec}) %>
                <% }); %> 
            </div> 

            <div id="dashboard_browse">
                <% documents.forEach((doc) => { %>
                    <%- include("partials/browse-listitem.ejs", {doc: doc})%>
                <% }); %>
            </div> 

        </main>
	</body>
</html>

