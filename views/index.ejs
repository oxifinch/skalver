<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8" />
		<title>Skalver | Dashboard</title>
		<meta name="description" content="Dashboard of Skalver web app"/>
		<meta name="viewport" content="width=device-width, initial-scale=1"/>
        <link rel="stylesheet" href="/css/style.css">
	</head>

	<body>
        <main>
            <%- include ("partials/dashboard-header.ejs") %>
            <div id="dashboard_sections">
                <% library.sections.forEach((section) => { %>
                    <%- include("partials/library-section.ejs", {section: section}) %>
                <% }); %> 
            </div> 

            <div id="dashboard_browse">
                <% library.books.forEach((book) => { %>
                    <%- include("partials/book-listitem.ejs", {book: book}) %>
                <% }); %>
            </div> 

        </main>
	</body>
</html>

